#include <stdio.h>
void main(void)
{
    long age, pw = 1234, password;

    printf("나이입력:");
    scanf_s("%ld", &age);

  if(age <= 15){
    printf("비밀번호 입력:");
    scanf_s("%d", &password);

  if(pw==password){
   puts("티비시청");
  }
  else{
    puts("비번 불일치");
    puts("부모님 허락필요");
    }
  }
  else{
    puts("시청허용");
    puts("채녈선택");
  }
}
